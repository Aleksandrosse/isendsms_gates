							PostData.Add('phone_code',SMS['Phone_Def']);
							PostData.Add('number',SMS['Phone_Number']);
							PostData.Add('sms_text',SMS['Text']);
							if IsAscii(SMS['Text']) then
								PostData.Add('translit','1');
							PostData.Add('private_key',SMS['Code']);
							PostData.Add('public_key',Html.RegExp('id=''public_key'' value=''(.*?)''',1));
						end;
					)
		</postdata>
