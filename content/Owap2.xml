<?xml version="1.0" encoding="UTF-8"?>
<gate>
	<name>Owap2</name>
	<caption>Owap2</caption>
	<type>SMS</type>
	<class>HTTP</class>
	<version>2</version>
	<priority>3</priority>
	<country>Россия</country>
	<prefixlist>
		<prefix start="+7 700 000" end="+7 799 999"/>
		<prefix start="+7 900 000" end="+7 999 999"/>
	</prefixlist>
	<info>
		<maxsymbols>140</maxsymbols>
		<maxunicodesymbols>140</maxunicodesymbols>
		<supportflash>0</supportflash>
		<supportdtdelivery>0</supportdtdelivery>
		<supportunicode>1</supportunicode>
		<supportcheckdelivery>0</supportcheckdelivery>
		<captcha>1</captcha>
		<gateurl>http://sms.owap.biz/wsms2/?xhtml&amp;y</gateurl>
	</info>
	<classprops>
		<encoding>UTF-8</encoding>
		<pageurl>http://sms.owap.biz/wsms2/?xhtml&amp;y</pageurl>
		<imageurl>eval('http://sms.owap.biz/wsms2/img.php?id='+Html.RegExp('img.php\?id=(.*?)&quot;',1))</imageurl>
		<posturl>eval('http://sms.owap.biz/wsms2/index.php?tmp='+Html.RegExp('index.php\?tmp=(.*?)&amp;',1)+'&amp;y')</posturl>
		<postdata>eval(
			begin
				PostData.Add('code',SMS['Code']);
				PostData.Add('phone',SMS['Phone_Def']+SMS['Phone_Number']);
				PostData.Add('text',SMS['Text']);
				PostData.Add('id',Html.RegExp('name=&quot;id&quot; value=&quot;(.*?)&quot;',1));
			end;
		)
		</postdata>
		<success>eval(Html.ExistsStr('Ваше сообщение отправлено'))</success>
		<error>eval(Html.RegExp('&lt;body&gt;&lt;div&gt;(.*?)&lt;br/&gt;',1))</error>
	</classprops>
</gate>