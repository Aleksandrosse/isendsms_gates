	<info>
		<maxsymbols>160</maxsymbols>
		<maxunicodesymbols>160</maxunicodesymbols>
		<supportflash>0</supportflash>
		<supportdtdelivery>0</supportdtdelivery>
		<supportunicode>0</supportunicode>
		<supportcheckdelivery>0</supportcheckdelivery>
		<captcha>0</captcha>
		<gateurl>http://sms.owap.biz/wses3/</gateurl>
	</info>
	<classprops>
		<encoding>UTF-8</encoding>
		<pageurl>http://sms.owap.biz/wses3/?y</pageurl>
		<posturl>eval('http://sms.owap.biz/wses3/index.php?tmp='+Html.RegExp('action=&quot;/wses3/index\.php\?tmp=(.*?)&amp;',1)+'&amp;y')</posturl>
		<postdata>eval(
			begin
				PostData.Add('number',SMS['Phone']);
				PostData.Add('owap',Html.RegExp('name=&quot;owap&quot; value=&quot;(.*?)&quot;',1));
				PostData.Add('text',SMS['Text']);
			end;
		)
		</postdata>
		<success>eval(Html.ExistsStr('Ваше сообщение отправлено'))</success>
		<error>eval(Html.RegExp('&lt;div&gt;(.*?)&lt;',1))</error>
	</classprops>
</gate>
